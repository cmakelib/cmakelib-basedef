
@PACKAGE_INIT@
FUNCTION(__INCLUDE_TARGETS_CONFIGS include_targets_names)
    FOREACH(file IN LISTS include_targets_names)
        CMAKE_PATH(GET file FILENAME filename)
        INCLUDE("${CMAKE_CURRENT_LIST_DIR}/${filename}")

        STRING(REGEX REPLACE "\\.cmake$" "" target_name "${filename}")
        GET_TARGET_PROPERTY(interface_sources ${target_name} CMDEF_INSTALL_INTERFACE_SOURCES)
        IF(interface_sources)
            TARGET_SOURCES(${target_name} INTERFACE $<BUILD_INTERFACE:"${interface_sources}">)
        ENDIF()
    ENDFOREACH()
ENDFUNCTION()

SET(include_targets_names "@PACKAGE_target_names@")
__INCLUDE_TARGETS_CONFIGS("${include_targets_names}")

SET_AND_CHECK(config_version_file "${CMAKE_CURRENT_LIST_DIR}/${CMAKE_FIND_PACKAGE_NAME}ConfigVersion.cmake")
INCLUDE("${config_version_file}")
